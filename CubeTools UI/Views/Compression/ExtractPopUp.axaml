<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="200"
        Width="300" Height="100"
        CanResize="False"
        x:Class="CubeTools_UI.Views.Compression.ExtractPopUp"
        Title="Extraction"
        Icon="../../Assets/CubeToolsIconsCompressed/Compression.ico"
        KeyDown="OnKeyPressedWindow">
        
        <Grid ColumnDefinitions="100,200">
                <Grid Grid.Column="0">
                        <Image Source="../../Assets/CubeToolsIcons/Compression.ico" Margin="10"/>
                </Grid>
                
                <Grid Grid.Column="1" RowDefinitions="50, 50">
                        <StackPanel Grid.Row="0" Name="ArchiveInformation"/>
                        <WrapPanel Grid.Row="1">
                                <Button Content="Extract" PointerPressed="Extract"/>
                                <Button Content="Cancel" PointerPressed="OnCancelPressed"/>
                        </WrapPanel>
                </Grid>
               
        </Grid>
        
</Window>
