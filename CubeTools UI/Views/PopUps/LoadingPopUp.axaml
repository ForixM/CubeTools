<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:popUps="clr-namespace:CubeTools_UI.ViewModels.PopUps"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="100"
        Width="300" Height="50"
        x:Class="CubeTools_UI.Views.PopUps.LoadingPopUp"
        Title="Loading" Closing="OnClosing">
    
    <Window.DataContext>
        <popUps:LoadingPopUpViewModel/>
    </Window.DataContext>
    
    <StackPanel>
        <TextBlock Name="OperationType" FontSize="15" Foreground="Black" Text="Process to operation" Height="30"/>
        <ProgressBar Foreground="Red" Name="Progress" Minimum="0" Maximum="100" Value="{Binding Progress}" Width="150" Height="8"/>
    </StackPanel>
</Window>