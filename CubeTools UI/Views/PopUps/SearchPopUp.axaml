<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="100"
        Width="200" Height="100"
        MinHeight="100" MinWidth="200"
        CanResize="False"
        x:Class="CubeTools_UI.Views.PopUps.SearchPopUp"
        Title="Search"
        Icon="\Assets/search.ico">
        
        <StackPanel Margin="10">
                <TextBox Watermark="*.txt" Name="TextEntered"/>
                <Button Margin="20 10 0 20" Content="Click to Search" Click="SearchClick"/>
        </StackPanel>
</Window>
