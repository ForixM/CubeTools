 <UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CubeTools_UI.Views.Settings.Generators.SingleObject.FtpServerObject">
	 
	 <StackPanel>
		 <TextBlock Name="Name" />
		 <WrapPanel>
			 <TextBlock Text="IP :"/>
			 <TextBox TextInput="OnIPModified" Name="Ip"/>
		 </WrapPanel>
		 <WrapPanel>
			 <TextBlock Text="Login : "/>
			 <TextBox  TextInput="OnLoginModified" Name="Login"/>
		 </WrapPanel>
		 <WrapPanel>
			 <TextBlock Text="Password : "/>
			 <TextBox TextInput="OnPasswordModified" Name="Password"/>
		 </WrapPanel>
		 <WrapPanel>
			 <TextBlock Text="Port : "/>
			 <TextBox TextInput="OnPortModified" Name="Port"/>
		 </WrapPanel>
	 </StackPanel>
 </UserControl>
