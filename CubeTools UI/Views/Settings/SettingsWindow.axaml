 <Window xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CubeTools_UI.Views.Settings.SettingsWindow">
	
	<Window.Styles>
		<StyleInclude Source="/Styles/Styles.axaml"/>
	</Window.Styles>
	 
	 <Grid>
		 <StackPanel>
			 <ComboBox>
				 <TextBlock Name="Theme1" Text="Light"/>
				 <TextBlock Name="Theme2" Text="Dark"/>
			 </ComboBox>
			 <ButtonSpinner AllowSpin="True"/>
		 </StackPanel>
	 </Grid>
</Window>
