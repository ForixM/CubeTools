<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:popUps="clr-namespace:Ui.Models.PopUps"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="100"
        Width="300" Height="50"
        Topmost="True"
        x:Class="Ui.Views.PopUps.LoadingPopUp"
        Title="Loading">
    
    <Window.DataContext>
        <popUps:LoadingPopUpModel/>
    </Window.DataContext>
    
    <StackPanel>
        <TextBlock Name="OperationType" FontSize="15" Foreground="Black" Text="Process to operation" Height="30"/>
        <ProgressBar Background="White" BorderBrush="Black" Foreground="Green" Name="ProgressBar" Minimum="0" Maximum="100" Width="150" Height="8"/>
    </StackPanel>
</Window>