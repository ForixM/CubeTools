 <UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Ui.Views.Settings.Generators.SingleObject.FtpSettingObject">
	 
	 <StackPanel>
		 <TextBox KeyDown="OnNameModified" Name="Name" />
		 <WrapPanel>
			 <TextBlock Text="IP :"/>
			 <TextBox KeyDown="OnIPModified" Name="Ip"/>
		 </WrapPanel>
		 <WrapPanel>
			 <TextBlock Text="Login : "/>
			 <TextBox KeyDown="OnLoginModified" Name="Login"/>
		 </WrapPanel>
		 <WrapPanel>
			 <TextBlock Text="Password : "/>
			 <TextBox KeyDown="OnPasswordModified" Name="Password"/>
		 </WrapPanel>
		 <WrapPanel>
			 <TextBlock Text="Port : "/>
			 <TextBox KeyDown="OnPortModified" Name="Port"/>
		 </WrapPanel>
	 </StackPanel>
 </UserControl>
